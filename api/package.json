{
  "name": "apollo-localhost",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "transpile": "babel src --out-dir build --ignore 'src/__tests__'",
    "start:dev": "nodemon build/index.js",
    "start": "NODE_ENV=dev npm-run-all transpile start:prod",
    "ss": "npm run clearDb && npm run seedDb && npm start",
    "start:prod": "NODE_ENV=prod node build/index.js",
    "testdbs": "docker-compose -f ../docker-compose.test.yml up",
    "test:local": "NODE_ENV=test jest --watch",
    "test": "NODE_ENV=test jest --watch",
    "test:cicd": "NODE_ENV=test jest",
    "seedDb": "NODE_ENV=dev node build/scripts/seedDb.js",
    "clearDb": "NODE_ENV=dev CMD_LINE=true node build/scripts/runClearDB.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "apollo-fetch": "^0.7.0",
    "apollo-server": "^2.18.2",
    "babel-jest": "^26.6.3",
    "dotenv": "^8.2.0",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "jest": "^26.6.3",
    "neo4j-driver": "^4.1.2",
    "nodemon": "^2.0.4",
    "path": "^0.12.7"
  },
  "jest": {
    "verbose": true,
    "roots": [
      "<rootDir>/src"
    ]
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}